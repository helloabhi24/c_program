#include<stdio.h>
#include<string.h>
int main()
{
    int a,b,c,d,e,f;
    for(int i=1;i<=999;i++){
    e=i%100;
    b=i/100;
    c=e/10;
    d=i%10;
        
        if(b==9||c==9||d==9)
        printf(" %d no. has 9\n",i);
    }
    
}









#include<stdio.h>
#include<string.h>
int main()
{
    int sum=0,n,i,a;
    printf("ENter a no.");
    scanf("%d",&a);
    i=a;
    while(i>0)
    {
        n=i%10;
        sum=sum+n;
        i=i/10;
        
    }
    printf(" %d is sum of %d\n",sum,a);
    
}
